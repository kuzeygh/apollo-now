{
  "name": "apollo-now",
  "version": 2,
  "builds": [
    { "src": "backend/src/index.js", "use": "@now/node-server" },
    { "src": "frontend/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/graphql", "dest": "backend/src/index.js" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}
